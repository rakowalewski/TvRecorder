@model IEnumerable<TvRecorderFinall.Models.Notification>



<table style="width:100%;"><tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr><tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr><tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr></table>
<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Sap </th>
        <th>Numer zgłoszenia </th>
        <th>Data wgrania </th>
        <th>Login</th>
        <th>Nazwa pliku</th>
        @*<th>Download</th>*@
    </tr>
    @foreach (var d in Model)
    {
        <tr>
            <td>@d.Sap</td>
            <td>@d.IdNotification</td>
            <td>@d.CreatedAt</td>
            <td>@d.Login</td>
            <td>@d.NameFile</td>


        </tr>
    }
</table>
